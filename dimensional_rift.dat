int currentCol=0
int arrayIndex=0

# Additional utility variables for swapping operations
int sourceRow=0
int sourceCol=0
int targetRow=0
int targetCol=0
int tempTileId=0
bool copyInProgress=false

# Generalized loop control variables
bool leftMap=true
bool leftArray=true
bool read=true

# Dimension tracking
bool inNormalDimension=true

# Task queue for sequential operations (enum values)
# 0 = done/no next task
# 1 = readRightHalfToArray (init sequence)
# 2 = loadRightArrayToLeftMap (switch to alternate dimension)
# 3 = loadLeftArrayToLeftMap (switch to normal dimension)
int nextTask=0

# Initialize all derived variables from mapRows and mapCols
init::;
halfCols=mapCols//2;
rightColStart=halfCols;
maxRowIndex=mapRows-1;
maxArrayIndex=mapRows*halfCols;
maxArrayIndex-=1;
nextTask=1;
readLeftHalfToArray;

readLeftHalfToArray::;
leftMap=true;
leftArray=true;
read=true;
generalizedLoop;

readRightHalfToArray::;
leftMap=false;
leftArray=false;
read=true;
generalizedLoop;

# Generalized loop for reading/writing map halves to/from arrays
generalizedLoop::;
((copyInProgress==false))initializeLoop;
((arrayIndex<=maxArrayIndex))[processArrayElement][completionHandler];

initializeLoop::;
currentRow=0;
arrayIndex=0;
copyInProgress=true;
((leftMap==true))currentCol=0;
((leftMap==false))currentCol=rightColStart;
((leftMap==true))maxCols=halfCols;
((leftMap==false))maxCols=mapCols;

processArrayElement::;
((read==true))readFromMapToArray;
((read==false))writeFromArrayToMap;
incrementAndAdvance;

readFromMapToArray::;
tempTileId=get(currentRow)(currentCol);
((leftArray==true))leftHalfTiles[arrayIndex]=tempTileId;
((leftArray==false))rightHalfTiles[arrayIndex]=tempTileId;

writeFromArrayToMap::;
((leftArray==true))tempTileId=leftHalfTiles[arrayIndex];
((leftArray==false))tempTileId=rightHalfTiles[arrayIndex];
place:currentRow,currentCol,tempTileId;

incrementAndAdvance::;
arrayIndex+=1;
currentCol+=1;
((currentCol>=maxCols))resetColAndAdvanceRow;
wait:0.0;
generalizedLoop;

resetColAndAdvanceRow::;
((leftMap==true))currentCol=0;
((leftMap==false))currentCol=rightColStart;
((currentRow<maxRowIndex))currentRow+=1;

completionHandler::;
copyInProgress=false;
((nextTask==1))readRightHalfToArray;
((nextTask==2))loadRightArrayToLeftMap;
((nextTask==3))loadLeftArrayToLeftMap;
nextTask=0;

# Swap dimensions of the left map area
swapDimensions::;
((inNormalDimension==true))[switchToAlternateDimension][switchToNormalDimension];

switchToAlternateDimension::;
nextTask=2;
inNormalDimension=false;
saveCurrentLeftMapToLeftArray;

switchToNormalDimension::;
nextTask=3;
inNormalDimension=true;
saveCurrentLeftMapToRightArray;

saveCurrentLeftMapToLeftArray::;
leftMap=true;
leftArray=true;
read=true;
generalizedLoop;

loadRightArrayToLeftMap::;
leftMap=true;
leftArray=false;
read=false;
generalizedLoop;

saveCurrentLeftMapToRightArray::;
leftMap=true;
leftArray=false;
read=true;
generalizedLoop;

loadLeftArrayToLeftMap::;
leftMap=true;
leftArray=true;
read=false;
generalizedLoop;

}
